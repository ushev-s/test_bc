{
  "components/cart/totals": "<ul class=\"cart-totals\">\n    <li class=\"cart-total cart-total--subtotal\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\n        </div>\n        <div class=\"cart-total-value\">\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                <span>{{cart.sub_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n    {{#if cart.gift_wrapping_cost.value}}\n        <li class=\"cart-total cart-total--giftWrapping\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cart.gift_wrapping_cost.formatted}}</span>\n            </div>\n        </li>\n    {{/if}}\n    {{#if cart.shipping_handling.show_estimator}}\n        <li class=\"cart-total cart-total--shippingEstimator\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.shipping'}}:</strong>\n            </div>\n            {{> components/cart/shipping-estimator cart.shipping_handling}}\n        </li>\n    {{/if}}\n    {{#each cart.taxes}}\n        <li class=\"cart-total cart-total--taxes\">\n            <div class=\"cart-total-label\">\n                <strong>{{name}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cost.formatted}}</span>\n            </div>\n        </li>\n    {{/each}}\n    {{#if cart.discount }}\n        <li class=\"cart-total cart-total--discount\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.discount'}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                {{cart.discount.formatted}}\n            </div>\n        </li>\n    {{/if}}\n    {{#if cart.coupons}}\n        {{#each cart.coupons}}\n            <li class=\"cart-total cart-total--coupons\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\n                    <div>\n                        <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\n                    </div>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{discount.formatted}}</span>\n                </div>\n            </li>\n        {{/each}}\n    {{else}}\n        <li class=\"cart-total cart-total--couponCode\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.coupon_code'}}:</strong>\n            </div>\n            {{> components/cart/coupon-input}}\n        </li>\n    {{/if}}\n    {{#if settings.gift_certificates_enabled}}\n        {{#each cart.gift_certificates}}\n            <li class=\"cart-total cart-total--giftCertificates\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\n                    <span>\n                        {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n                    </span>\n                </div>\n                <div class=\"cart-total-value\">\n                    {{used.formatted}}\n                </div>\n            </li>\n        {{/each}}\n        <li class=\"cart-total cart-total--giftCertificates\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\n            </div>\n            {{> components/cart/gift-certificate-input}}\n        </li>\n    {{/if}}\n    <li class=\"cart-total cart-total--grandTotal\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\n        </div>\n        <div class=\"cart-total-value cart-total-grandTotal\">\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                <span>{{cart.grand_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n</ul>\n",
  "components/common/login-for-pricing": "<a class=\"price--login\" href=\"/login.php\" translate>{{lang 'common.login_for_pricing'}}</a>\n",
  "components/cart/shipping-estimator": "{{inject 'shippingCountryErrorMessage' (lang 'cart.shipping_estimator.empty_country_error')}}\n{{inject 'shippingProvinceErrorMessage' (lang 'cart.shipping_estimator.empty_province_error')}}\n\n{{#if shipping_cost}}\n    <div class=\"cart-total-value\">\n        <div class=\"subtotal shipping-estimate-show\">\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\n        </div>\n    </div>\n{{else}}\n    <div class=\"cart-total-value\">\n        <button class=\"shipping-estimate-show\">{{lang 'cart.shipping_estimator.add_info'}}</button>\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\n    </div>\n{{/if}}\n\n<div class=\"shipping-estimator u-hiddenVisually\">\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\"></div>\n</div>\n",
  "components/cart/coupon-input": "<div class=\"cart-total-value\">\n\n    <button class=\"coupon-code-add\">{{lang 'cart.coupons.add_coupon'}}</button>\n\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form coupon-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n",
  "components/cart/gift-certificate-input": "<div class=\"cart-total-value\">\n\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\n\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n"
}