{"components/papa-supermarket/also-bought/also-bought":"<div class=\"productView-alsoBought productView-alsoBought--{{theme_settings.alsobought_position}} u-hiddenVisually\" data-also-bought>\n    <div class=\"productView-alsoBought-heading\">{{lang 'also_bought.heading'}}</div>\n    <div class=\"productView-alsoBought-thumbnails\" data-thumbnails></div>\n    <div class=\"productView-alsoBought-buttons\">\n        <button class=\"button button--small button--primary\" data-add-all>{{lang 'also_bought.add_all'}}</button>\n        <button class=\"button button--small button--primary\" data-add-to-cart>{{lang 'also_bought.add_selected_to_cart'}}</button>\n    </div>\n    <div class=\"productView-alsoBought-list\">\n        {{#if theme_settings.alsobought_product '==' 'related'}}\n            {{#each product.related_products}}\n                <div class=\"productView-alsoBought-item\" data-product-id=\"{{id}}\"></div>\n                {{inject 'enableAlsoBought' true}}\n            {{/each}}\n        {{else if theme_settings.alsobought_product '==' 'similar'}}\n            {{#each product.similar_by_views}}\n                <div class=\"productView-alsoBought-item\" data-product-id=\"{{id}}\"></div>\n                {{inject 'enableAlsoBought' true}}\n            {{/each}}\n        {{else}}\n            {{#each product.custom_fields}}\n                {{#if name '===' '__alsobought'}}\n                    <div class=\"productView-alsoBought-item\" data-product-id=\"{{value}}\"></div>\n                    {{inject 'enableAlsoBought' true}}\n                {{/if}}\n            {{/each}}\n        {{/if}}\n    </div>\n</div>\n"}
