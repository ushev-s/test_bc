{"components/category/grid-list":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--mode\">\n    <legend class=\"is-srOnly\">{{lang 'common.sorter.columns'}}</legend>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"mode\">{{lang 'common.sorter.columns'}}:</label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"1\" id=\"mode_1{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_1{{id_suffix}}\"><span class=\"form-option-variant\" title=\"1\">1</span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"2\" id=\"mode_2{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid2'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_2{{id_suffix}}\"><span class=\"form-option-variant\" title=\"2\">2</span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"3\" id=\"mode_3{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid3'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_3{{id_suffix}}\"><span class=\"form-option-variant\" title=\"3\">3</span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"4\" id=\"mode_4{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_4{{id_suffix}}\"><span class=\"form-option-variant\" title=\"4\">4</span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"6\" id=\"mode_6{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid6'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_6{{id_suffix}}\"><span class=\"form-option-variant\" title=\"6\">6</span></label>\n        {{#if theme_settings.show_bulk_order_mode}}\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"bo\" id=\"mode_bo{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'listbo'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_bo{{id_suffix}}\"><span class=\"form-option-variant\" title=\"{{lang 'bulk_order.icon_title'}}\"><i class=\"fa fa-th-list\"></i></span></label>\n        {{/if}}\n\n        <!--\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"grid\" id=\"mode_grid{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_grid{{id_suffix}}\" aria-label=\"Grid\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"list\" id=\"mode_list{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\n        <label class=\"form-option\" for=\"mode_list{{id_suffix}}\" aria-label=\"List\"><span class=\"form-option-variant\" title=\"List\"><i class=\"fa fa-th-list\" aria-hidden=\"true\"></i></span></label>\n        -->\n    </div>\n</fieldset>","components/category/products-per-page":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--productsPerPage\">\n    <legend class=\"is-srOnly\">{{lang 'common.sorter.products_per_page'}}</legend>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"limit-{{filter_id}}\">{{lang 'common.sorter.products_per_page'}}:</label>\n        <select class=\"form-select\" name=\"limit\" id=\"limit-{{filter_id}}\">\n            <option value=\"8\" {{#if products_per_page '==' 8}}selected{{/if}}>8</option>\n            <option value=\"12\" {{#if products_per_page '==' 12}}selected{{/if}}>12</option>\n            <option value=\"16\" {{#if products_per_page '==' 16}}selected{{/if}}>16</option>\n            <option value=\"20\" {{#if products_per_page '==' 20}}selected{{/if}}>20</option>\n            <option value=\"40\" {{#if products_per_page '==' 40}}selected{{/if}}>40</option>\n            <option value=\"100\" {{#if products_per_page '==' 100}}selected{{/if}}>100</option>\n        </select>\n    </div>\n</fieldset>\n\n","components/category/sort-box":"<fieldset class=\"form-fieldset actionBar-section actionBar-section--sort\">\n    <legend class=\"is-srOnly\">{{lang 'common.sorter.sort_by'}}</legend>\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort-{{filter_id}}\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select\" name=\"sort\" id=\"sort-{{filter_id}}\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n</fieldset>\n\n","components/products/filter":"<div class=\"emthemesModez-productsFilter {{#or category.faceted_search_enabled brand.faceted_search_enabled product_results.faceted_search_enabled}}emthemesModez-productsFilter--faceted{{/or}}\">\n    <a href=\"#emthemesModez-productsFilter-form\" role=\"button\" class=\"button emthemesModez-productsFilter-toggle\" aria-label=\"Options\" data-collapsible><i class=\"fa fa-cog\" aria-hidden=\"true\"></i></a>\n    <form class=\"actionBar\" id=\"emthemesModez-productsFilter-form\" method=\"get\" data-sort-by>\n        {{> components/category/sort-box sort=sort}}\n        {{> components/category/products-per-page products_per_page=products_per_page}}\n        {{> components/category/grid-list}}\n    </form>\n</div>"}
