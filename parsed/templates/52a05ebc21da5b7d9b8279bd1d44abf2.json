{
  "components/emthemes-modez/newsletter/section": "<div class=\"emthemesModez-newsletterSection{{#if theme_settings.social_icon_placement_bottom}} emthemesModez-newsletterSection--hasSocials{{/if}}\">\n    <article class=\"emthemesModez-newsletterForm\" data-section-type=\"newsletterSubscription\">\n        {{> components/common/subscription-form}}\n    </article>\n</div>",
  "components/common/subscription-form": "<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n{{#if theme_settings.subscribe_intro '!==' ''}}\n    <p>{{theme_settings.subscribe_intro}}</p>\n{{else if settings.show_newsletter_summary}}\n    <p>{{settings.newsletter_summary}}</p>\n{{/if}}\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input form-prefixPostfix-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\" aria-required=\"true\" required>\n                <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n        </div>\n    </fieldset>\n</form>\n"
}