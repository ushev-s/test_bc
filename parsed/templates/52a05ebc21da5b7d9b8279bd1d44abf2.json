{"components/common/subscription-form":"<p class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</p>\r\n{{#if theme_settings.subscribe_intro '!==' ''}}\r\n    <p>{{theme_settings.subscribe_intro}}</p>\r\n{{else if settings.show_newsletter_summary}}\r\n    <p>{{settings.newsletter_summary}}</p>\r\n{{/if}}\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <legend class=\"is-srOnly\">Subscription form</legend>\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input form-prefixPostfix-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n","components/emthemes-modez/newsletter/section":"<div class=\"emthemesModez-newsletterSection{{#if theme_settings.social_icon_placement_bottom}} emthemesModez-newsletterSection--hasSocials{{/if}}\">\n    <article class=\"emthemesModez-newsletterForm\" data-section-type=\"newsletterSubscription\">\n        {{> components/common/subscription-form}}\n    </article>\n</div>"}
