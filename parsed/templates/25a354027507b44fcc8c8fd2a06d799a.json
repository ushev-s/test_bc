{"components/account/add-wishlist":"<div class=\"wishlist-add\">\n    <header class=\"wishlist-header\">\n        {{#if forms.wishlist.id}}\n            <h1>{{lang 'account.wishlists.edit'}}</h1>\n        {{else}}\n            <h1>{{lang 'account.wishlists.new'}}</h1>\n            <p>{{lang 'account.wishlists.tag_line'}}</p>\n        {{/if}}\n        {{#if forms.wishlist.errors}}\n            {{> components/common/alert-error forms.wishlist.errors}}\n        {{/if}}\n    </header>\n    <form action=\"{{#if forms.wishlist.id}}{{urls.account.wishlists.edit}}{{else}}{{urls.account.wishlists.add}}&product_id={{forms.wishlist.product_id}}{{/if}}\" class=\"form wishlist-form\" method=\"post\">\n        <input type=\"hidden\" name=\"submit\">\n\n        <div class=\"form-field\">\n            <label class=\"form-label\" for=\"wishlistname\">\n                {{lang 'forms.wishlist.name'}}\n                <small>{{lang 'common.required' }}</small>\n            </label>\n            <input class=\"form-input\" name=\"wishlistname\" id=\"wishlistname\" value=\"{{forms.wishlist.name}}\">\n        </div>\n        <div class=\"form-field\">\n            <input type=\"checkbox\" class=\"form-checkbox\" name=\"publicwishlist\" id=\"publicwishlist\" {{#if forms.wishlist.is_public}}checked{{/if}}>\n            <label class=\"form-label\" for=\"publicwishlist\">{{lang 'forms.wishlist.public' }}</label>\n\n        </div>\n        <div class=\"form-actions\">\n            {{#if forms.wishlist.id}}\n                <input type=\"submit\" class=\"button button--primary\" value=\"{{lang 'account.wishlists.save'}}\">\n                <input type=\"hidden\" name=\"wishlistid\" value=\"{{forms.wishlist.id}}\">\n            {{else}}\n                <input type=\"submit\" class=\"button button--primary\"\n                       value=\"{{lang 'account.wishlists.create'}}\">\n            {{/if}}\n        </div>\n    </form>\n\n</div>\n","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"}
