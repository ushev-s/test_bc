{"components/account/edit-account":"<form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\n    {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\n    {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\n    {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\n    {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-row form-row--half\">\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_firstname\">\n                    {{lang 'account.settings.first_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{forms.edit_account.first_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_lastname\">\n                    {{lang 'account.settings.last_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{forms.edit_account.last_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_companyname\">\n                    {{lang 'account.settings.company' }}\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{forms.edit_account.company_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"account_phone\">\n                    {{lang 'account.settings.phone' }}\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"account_phone\" id=\"account_phone\" value=\"{{forms.edit_account.phone}}\">\n            </div>\n            {{#each forms.edit_account.fields}}\n                {{{dynamicComponent 'components/common/forms'}}}\n            {{/each}}\n        </div>\n    </fieldset>\n    <div class=\"form-actions\">\n        <button class=\"button button--primary\">{{lang 'account.settings.update_details'}}</button>\n    </div>\n</form>\n","components/common/forms/checkbox":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\n        {{#if required}}\n            <small>{{lang 'common.required' }}</small>\n        {{/if}}\n    </label>\n    {{#if checkboxes}}\n        {{#each checkboxes}}\n            <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{name}}\" data-label=\"{{label}}\" class=\"form-checkbox\" {{#if checked}}checked{{/if}} {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/common/forms/date":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}\" >{{label}}\n    {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}</label>\n    <div class=\"form-row form-row--third\">\n        <div class=\"form-field\">\n            <select class=\"form-select\" name=\"{{month.name}}\" id=\"{{id}}_month\" data-label=\"month\"  aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n                {{#each month.items}}\n                    <option {{#if ../month.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\n                {{/each}}\n            