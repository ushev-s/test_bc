{
  "components/amp/common/footer": "<footer class=\"amp-footer\" role=\"contentinfo\">\n\n    {{#if theme_settings.footer_show_aboutus}}\n    <article class=\"amp-footer-col amp-footer-col--about\" data-section-type=\"storeInfo\">\n        <h5 class=\"amp-footer-heading\">{{> components/amp/common/store-logo}}</h5>\n        {{#if theme_settings.footer_show_contact}}\n            <address>{{nl2br settings.address}}</address>\n            {{#if settings.phone_number}}\n                <strong>{{lang 'footer.call_us' phone_number=settings.phone_number}}</strong>\n            {{/if}}\n        {{/if}}\n    </article>\n    {{/if}}\n\n    <article class=\"amp-footer-col amp-footer-col--pages\" data-section-type=\"footer-webPages\">\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.navigate'}}</h5>\n        <ul class=\"amp-footer-list\">\n            {{#each pages}}\n            <li>\n                <a href=\"{{url}}\">{{name}}</a>\n            </li>\n            {{/each}}\n            <li>\n                <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\n            </li>\n        </ul>\n    </article>\n\n    <article class=\"amp-footer-col amp-footer-col--categories\" data-section-type=\"footer-categories\">\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.categories'}}</h5>\n        <ul class=\"amp-footer-list\">\n            {{#each categories}}\n                <li>\n                    <a href=\"{{url}}\">{{name}}</a>\n                </li>\n            {{/each}}\n        </ul>\n    </article>\n\n    <article class=\"amp-footer-col amp-footer-col--brands\" data-section-type=\"footer-brands\">\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.brands'}}</h5>\n        <ul class=\"amp-footer-list\">\n            {{#each shop_by_brand}}\n                <li>\n                    <a href=\"{{url}}\">{{name}}</a>\n                </li>\n            {{/each}}\n            <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n        </ul>\n    </article>\n\n    <article class=\"amp-footer-copyright\">\n        {{#if theme_settings.show_powered_by}}\n            <p>{{lang 'footer.powered_by'}} <a href=\"http://bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\n        {{/if}}\n        {{#if theme_settings.show_copyright_footer}}\n            <p class=\"powered-by\">&copy; {{settings.store_name}} </p>\n        {{/if}}\n    </article>\n</footer>\n",
  "components/amp/common/store-logo": "<a href=\"{{urls.home}}\" class=\"amp-header-link\">\n    {{#if settings.store_logo.image}}\n        <amp-img src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n            {{#if theme_settings.logo_size '===' 'original' }}\n                width=\"100\"\n                height=\"50\"\n            {{else}}\n                width=\"{{first (split theme_settings.logo_size 'x')}}\"\n                height=\"{{last (split theme_settings.logo_size 'x')}}\"\n            {{/if}}\n            alt=\"{{settings.store_logo.title}}\">\n        </amp-img>\n    {{else}}\n        <span class=\"amp-header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"
}