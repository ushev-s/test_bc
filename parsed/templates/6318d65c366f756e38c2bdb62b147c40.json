{
  "components/emthemes-modez/banners/banner": "\n<div class=\"emthemesModez-banner\">\n\t<div class=\"emthemesModez-banner-container\">\n\t\t<figure class=\"emthemesModez-banner-figure\">\n\t\t\t{{#if image '!=' ' '}}\n\t\t\t<a href=\"{{url}}\"><img class=\"emthemesModez-banner-image\" src=\"{{image}}\" alt=\"{{title}}\" title=\"{{title}}\" /></a>\n\t\t\t{{/if}}\n\t\t</figure>\n\n\t\t\n\t\t<figcaption class=\"emthemesModez-banner-figcaption {{#if title '==' ' '}}{{#if text '==' ' '}}{{#if button '==' ' '}}emthemesModez-banner-figcaption--empty{{/if}}{{/if}}{{/if}}\">\n\t\t\t<div class=\"emthemesModez-banner-figcaption-body\">\n\t\t\t\t{{#if title '!=' ' '}}\n\t\t\t\t<h4 class=\"emthemesModez-banner-figcaption-title\">{{title}}</h4>\n\t\t\t\t{{/if}}\n\n\t\t\t\t{{#if text '!=' ' '}}\n\t\t\t\t<div class=\"emthemesModez-banner-figcaption-text\">{{text}}</div>\n\t\t\t\t{{/if}}\n\n\t\t\t\t{{#if button '!=' ' '}}\n\t\t\t\t<a href=\"{{url}}\" class=\"button button--primary button--small emthemesModez-banner-figcaption-button\">{{button}}</a>\n\t\t\t\t{{/if}}\n\t\t\t</div>\n\t\t</figcaption>\n\t</div>\n</div>"
